{{!
    @template format_tiles/progress_spacer

    Spacer.

    Example context (json):
    {
        "hastilephoto": true,
        "showprogressphototiles": true,
        "progress": {"numOutOf": 10, "numComplete": 5},
        "usetooltips": false
    }
}}

{{#hastilephoto}}
    {{#showprogressphototiles}}
        {{#progress}}
            <div class="completion-spacer float-right"></div>
        {{/progress}}
    {{/showprogressphototiles}}
{{/hastilephoto}}
{{^hastilephoto}}
    {{#progress}}
        <div class="completion-spacer float-right"></div>
    {{/progress}}
{{/hastilephoto}}